[testing]
testing-server ansible_host=oo-acs-testing ansible_user=root

[staging]
staging-server ansible_host=oo-acs-staging ansible_user=root

[production]
production-server ansible_host=oo-acs-production ansible_user=root

[all:vars]
ansible_python_interpreter=/usr/bin/python3
repository_url=https://github.com/unixthat/OO-ACS.git
